<h1>All Posts</h1>
<%= link_to 'New Post', new_post_path %>
<ul>
  <% @posts.each do |post| %>
    <li>
      <strong><%= link_to post.title, post %></strong>
      <%= link_to 'Edit', edit_post_path(post) %> |
      <%= link_to 'Delete', post, method: :delete, data: { confirm: 'Are you sure?' } %>
    </li>
  <% end %>
</ul>
